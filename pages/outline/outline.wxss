/* pages/outline/outline.wxss */
page{
  height: 100%;
  padding:0;
}
.outlinebj{
  width:100%;
  height: 100%;
  background-size: 100% 100%;
  padding: 80rpx 15px 0 15px;
  box-sizing:border-box;
}
.Title{
  display:block;
  text-align: center;
  font-size:50rpx;
  color:#221815;
}
.chopsticks{
  display: block;
  height: 232rpx;
  background-size:100% 100%;
  margin:25rpx auto 0;
}
.gameContent text{
  font-size:34rpx;
  color:#221815;
  line-height:60rpx;
}
swiper{
  height: 100%;
}
.pageBtn{
  position: relative;
  bottom:120px;
  display: flex;
  justify-content:space-between;
  padding: 0 100rpx;
  box-sizing: border-box;
}
.pageBtn image{
  width:67rpx;
  height:54rpx;
}